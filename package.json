{
  "name": "vote-box-io",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:VoteBoxIO/app.git"
  },
  "author": "Sergei Stepanenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VoteBoxIO/app/issues"
  },
  "homepage": "https://github.com/VoteBoxIO",
  "description": "",
  "scripts": {
    "generate-svgr": "npx @svgr/cli --typescript --no-index --no-svgo --out-dir src/svgr -- src/svg",
    "tsc": "bash -c tsc",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "development-https": "NODE_ENV=development HTTPS=true webpack serve --config webpack.config.ts",
    "development": "NODE_ENV=development webpack serve --config webpack.config.ts",
    "build": "rm -rf dist && npm run tsc && NODE_ENV=production webpack --config webpack.config.ts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,scss,md}": [
      "npm run format"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run tsc"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@orbs-network/ton-access": "2.3.3",
