    "@types/webpack-env": "1.18.5",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "@wyw-in-js/babel-preset": "0.5.4",
    "@wyw-in-js/webpack-loader": "0.5.4",
    "babel-loader": "9.1.3",
    "cors": "2.8.5",
